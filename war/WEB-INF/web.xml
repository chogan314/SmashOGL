<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	
    <servlet>
        <servlet-name>CreateUserServlet</servlet-name>
        <servlet-class>com.coryhogan.smashogl.server.servlets.CreateUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.coryhogan.smashogl.server.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnterQueueServlet</servlet-name>
        <servlet-class>com.coryhogan.smashogl.server.servlets.EnterQueueServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>CreateUserServlet</servlet-name>
        <url-pattern>/createUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnterQueueServlet</servlet-name>
        <url-pattern>/enterQueue</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
    
</web-app>